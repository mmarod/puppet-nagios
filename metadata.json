{
  "name": "mmarod-nagios",
  "version": "0.0.1",
  "author": "Michael Marod",
  "summary": "Puppet nagios module",
  "license": "Apache-2.0",
  "source": "https://github.com/mmarod/puppet-nagios.git",
  "project_page": "http://github.com/mmarod/puppet-nagios",
  "issues_url": "https://github.com/mmarod/puppet-nagios/issues",
  "dependencies": [
